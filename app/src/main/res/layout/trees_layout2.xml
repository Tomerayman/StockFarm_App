<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:id="@+id/trees_layout2"
    android:layout_height="match_parent">


    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/tree2"
        android:layout_width="160dp"
        android:layout_height="200dp"
        android:scaleType="fitCenter"
        app:lottie_scale="0.4"
        android:translationX="-100dp"
        android:translationY="-70dp"
        app:lottie_speed="0.21"
        app:layout_constraintBottom_toBottomOf="@id/bottomGuide"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/tree2" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/tree3"
        android:layout_width="160dp"
        android:layout_height="200dp"
        android:scaleType="fitCenter"
        app:lottie_scale="0.4"
        android:translationX="50dp"
        android:translationY="-70dp"
        app:lottie_speed="0.25"
        app:layout_constraintBottom_toBottomOf="@id/bottomGuide"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/tree2" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/tree1"
        android:layout_width="160dp"
        android:layout_height="200dp"
        android:scaleType="fitCenter"
        app:lottie_scale="0.4"
        android:translationX="-30dp"
        android:translationY="-30dp"
        app:lottie_speed="0.30"
        app:layout_constraintBottom_toBottomOf="@id/bottomGuide"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/tree2" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/tree4"
        android:layout_width="160dp"
        android:layout_height="200dp"
        android:scaleType="fitCenter"
        app:lottie_scale="0.4"
        android:translationX="-130dp"
        android:translationY="30dp"
        app:lottie_speed="0.2"
        app:layout_constraintBottom_toBottomOf="@id/bottomGuide"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/tree2" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/tree5"
        android:layout_width="160dp"
        android:layout_height="200dp"
        android:scaleType="fitCenter"
        app:lottie_scale="0.4"
        android:translationX="55dp"
        android:translationY="25dp"
        app:lottie_speed="0.24"
        app:layout_constraintBottom_toBottomOf="@id/bottomGuide"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/tree2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/bottomGuide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.88" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/lowLeftGuide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.17" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/highLeftGuide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.22" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/lowRightGuide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.82"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/highRightGuide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.77" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/topGuide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.67" />


</androidx.constraintlayout.widget.ConstraintLayout>